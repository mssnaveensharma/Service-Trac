   <div class="container">

    <div class="serch-fld">
          <label class="margin-top">Service Alert </label>
           <span class="align-right">
              <input class="form-control" type="search">
                <input value="" class="search-submit" type="submit" />
            </span>
           
        </div>  
<h1>Listing Electronics</h1>
        <div class="table-responsive">
          <table class="table-striped table-bordered table-condensed" width="100%">
  <thead>
    <tr>
      <th>EORB No</th>
      <th>Make</th>
      <th>Modal</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @electronics.each do |electronic| %>
      <tr>
        <td><%= electronic.s_no %></td>
        <td><%= electronic.make %></td>
        <td><%= electronic.model %></td>
        <!-- <td><%= link_to 'Show', electronic %></td> -->
        <td><%= link_to 'Edit', edit_electronic_path(electronic) %>
        <%= link_to 'Destroy', electronic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Electronic', new_electronic_path %>

</div>
