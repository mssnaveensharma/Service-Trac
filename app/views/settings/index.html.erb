<div class="srvc-cntr-fld"> 
<div class="srvc-frm">    
<% @users.each do |user| %>

  <%#=debug user %>
    <form method="POST" action="/updateProfile">
	<div class="form-group">
		<label>Email</label>
		<input name="email" type="email" readonly="readonly" value="<%= user.email %>"><br>
	</div>
	  <div class="form-group">
      <label>First Name</label>
      <input name="FirstName" type="text" value="<%= user.FirstName %>"><br>
	  </div>
	  <div class="form-group">
	  <label>Last Name</label>
      <input name="LastName" name="text" value="<%= user.LastName %>"><br>
	  </div>
	  <div class="form-group">
      <label>Password</label>
      <input type="password" name="password" ><br>
	  </div>
	  <div class="form-group">
      <label>Update my password</label>
      <input name="check" type="checkbox" value="1"><br>
	  </div>
      <input type="submit" name="commit" value="Update">
    </form>
  
<% end %>


