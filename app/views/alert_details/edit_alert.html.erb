    <div class="container main-padding">

    <div class="serch-fld">
          <label>Service Alert </label>
        </div>  
    
        <div>
        <ol class="breadcrumb">
      
      <li class="active">>> Main</li>
    </ol>
    	<form method="POST" action="/edit_alert">
    		<% @alert_detail = ServiceAlert.where(:id => params[:id]) %>
    			<%=debug @alert_detail %>
    			<% @alert_detail.each do |alert| %>
					<% @user = User.where(:id => alert.id) %>    			
						<% @user.each do |userInfo| %>
							<label>Contact#</label>
							<input type="text" name="contact" value="<%= userInfo.Contact %>"
						<% end %>
    			<% end %>
    	</form>
    </div>